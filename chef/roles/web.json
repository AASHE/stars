{
    "name": "web",
    "chef_type": "role",
    "json_class": "Chef::Role",
    "override_attributes": {
        "webapp_paths": {
            "webapp_path": "/var/www/stars/",
            "media_symlink_root": "/var/www/stars/media/",
            "media_links": {
                "/var/www/stars/src/current/stars/static/": "static",
                "/var/www/stars/src/current/parts/django/django/contrib/admin/media/": "admin"
            }
        },
        "nginx": {
            "sites": [
                {
                    "name": "stars",
                    "ssl_root": "/var/www/stars/ssl/",
                    "ssl_cert": "stars_combined.crt",
                    "ssl_key": "stars.aashe.org.key"
                }
            ]
        }
    },
    "description": "The STARS database server",
    "run_list": [
        "recipe[web::default]",
        "recipe[webapp_paths::default]",
        "recipe[web::memcached]",
        "recipe[web::apache2]",
        "recipe[web::celery]",
        "recipe[nginx::default]"
    ]
}